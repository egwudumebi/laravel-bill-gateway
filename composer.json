{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "egwudumebi/laravel-bill-gateway",
    "description": "Multi-provider bill payments gateway for Laravel (airtime, data, TV, power, utilities).",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "billing",
        "bills",
        "airtime",
        "data",
        "power",
        "tv",
        "payments"
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Aelura\\BillGateway\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aelura\\BillGateway\\BillGatewayServiceProvider"
            ],
            "aliases": {
                "BillGateway": "Aelura\\BillGateway\\Facades\\BillGateway"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
